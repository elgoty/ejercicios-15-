//20 
{
//costo terreno 2.0
let com = 750
let area = 900
if (area> 500 ){
  console.log("tienes un descuento de 10%")
  let preciod = (com*area)/10*9
  console.log(preciod)
}else {
  console.log("el precio es normal")
  let precio = com*area
  console.log(precio)
}
}
